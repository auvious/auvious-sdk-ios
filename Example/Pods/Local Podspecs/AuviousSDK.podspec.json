{
  "name": "AuviousSDK",
  "version": "1.0.7",
  "summary": "AuviousSDK makes it easy to use Auvious services in your app.",
  "description": "AuviousSDK makes it easy to use Auvious services in your app. Add now video call,\nmultiparty video conferencing, snapshot, and many more things are coming your way!",
  "homepage": "https://gitlab.auvious.com/auvious/api/auvious-sdk-ios.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Epi Vou": "pimenas@gmail.com"
  },
  "source": {
    "git": "https://gitlab.auvious.com/auvious/api/auvious-sdk-ios.git",
    "tag": "1.0.7"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source_files": "AuviousSDK/Classes/**/*",
  "resources": "AuviousSDK/Assets/*.png",
  "resource_bundles": {
    "AuviousSDK": [
      "Assets.xcassets"
    ]
  },
  "dependencies": {
    "Alamofire": [
      "~> 4.7"
    ],
    "SwiftyJSON": [
      "~> 5.0"
    ],
    "AuviousGoogleWebRTC": [
      "~> 84.4147.2"
    ],
    "MQTTClient/Websocket": [
      "~> 0.15"
    ]
  },
  "swift_versions": "4.2",
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO",
    "GCC_PREPROCESSOR_DEFINITIONS": "AUVIOUSSDK",
    "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "$(inherited) AUVIOUSSDK"
  },
  "swift_version": "4.2"
}
